<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_brew_year" model="ir.sequence">
            <field name="name">Brew year sequence</field>
            <field name="code">brew.year.sequence</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field eval="True" name="use_date_range" />
            <field name="padding">3</field>
        </record>

        <record id="sequence_brew_request" model="ir.sequence">
            <field name="name">Brew request sequence</field>
            <field name="code">brew.request.sequence</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding">3</field>
        </record>

        <record id="sequence_brew_declaration" model="ir.sequence">
            <field name="name">Brew declaration sequence</field>
            <field name="code">brew.declaration.sequence</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
